{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/explore", "dest": "/api/generateExplore.js" },
    { "src": "/api/overpoweredbot", "dest": "/api/overpoweredbot.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
